<div>
  <%= photo.get("time_ago") %> ago
  <br>
  created at: <%= photo.get("created_at") %>
  <br>
  author: <%= photo.get('author_id') %>
  <br>
  <%= photo.cloudinaryUrl({
    gravity: "custom",
    crop: 'thumb',
    width: photo.width(),
    height: photo.height()
  }) %>
</div>

<div class="likes">

</div>

<div class="title">
</div>

<ul class="comments list-group">
</ul>

<% if (photo.isAuthor()) { %>
  <br>
  <button type="button" class="delete-photo btn btn-danger" >Delete</button>
<% } %>
